\begin_layout Standard
Por otra parte en las tabla 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:atributosBasicosConexionesTCP"

\end_inset

, 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:atributosEspeciales"

\end_inset

 y 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:atributosConVentana2segundos"

\end_inset

 se especifícan los atributos divididos por los atributos básicos de las
 conexiones TCP, los atributos especiales y los atributos con ventana de
 dos segundos.
\end_layout

